{
  "bounds": <%= bounds %>,
  "center": <%= center %>,
  "format": "png8",
  "interactivity": false,
  "minzoom": <%= @minzoom %>,
  "maxzoom": <%= @maxzoom %>,
  "srs": "+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over",
  "Stylesheet": ["style.mss"],
  "Layer": [
    {
      "geometry": "polygon",
      "extent": <%= extent %>,
      "id": "<%= @habitat %>",
      "class": "",
      "Datasource": { "file": "<%= CartoDb.url_for cartodb_query %>" },
      "srs-name": "WGS84",
      "srs": "+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs",
      "advanced": {},
      "name": "<%= @habitat %>"
    }
  ],
  "scale": 1,
  "metatile": 4,
  "name": "",
  "description": ""
}

