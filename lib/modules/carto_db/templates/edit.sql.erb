UPDATE <%= table_name %> AS t
SET age = <%= validation.age.to_i %>,
  area_id = <%= validation.area_id.to_i %>,
  density = <%= validation.density.to_i %>,
  knowledge = '<%= validation.knowledge %>',
  notes = '<%= validation.notes %>',
  condition = <%= validation.condition %>
WHERE edit_phase = <%= validation.id %>;
