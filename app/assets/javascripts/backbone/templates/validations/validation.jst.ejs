<td><%= recorded_at %></td>
<td><% if(typeof user !== 'undefined') { %><a href="/users/<%= user.id %>"><%= user.email %><% } else { %>-<% } %></td>
<td><%= action.charAt(0).toUpperCase() + action.slice(1) %></td>
<td><%= habitat.replace(/_/g, ' ').replace(/(\w+)/g, function(match) {return match.charAt(0).toUpperCase() + match.slice(1)}) %></td>

<td><a href="#/<%= id %>">View</td>
<td><a href="#/<%= id %>/edit">Edit</a></td>

<% if( this.collection.isLatest(this.model) ) { %>
<td id="<%= id %>_delete" class="deleteme">Delete</td>
<% }
else { %><td></td><% } %>

